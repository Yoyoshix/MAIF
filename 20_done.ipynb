   "source": [
    "def fact_digit_sum(number):\n",
    "    fact = 1\n",
    "    for i in range(2,number+1):\n",
    "        fact *= i\n",
    "    total = 0\n",
    "    for i in str(fact):\n",
    "        total += int(i)\n",
    "    return total"
   ]
